# Google Cloud credentials and configuration
GOOGLE_APPLICATION_CREDENTIALS=./service-account.json
VERTEX_AI_PROJECT_ID=your-project-id
VERTEX_AI_LOCATION=us-central1

# Application directories
INPUT_DIR=./data
OUTPUT_DIR=./output
HCC_CODES_PATH=./data/HCC_relevant_codes.csv

# RabbitMQ configuration
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_QUEUE=document-events

# Storage watcher configuration
STORAGE_TYPE=local
WATCH_PATH=./data
WATCH_INTERVAL=10.0
FILE_PATTERNS=*.txt,*.pdf

# Logging
LOG_LEVEL=INFO

# Note: Replace the placeholders with your actual values.
# DO NOT commit your actual .env file with secrets to version control.